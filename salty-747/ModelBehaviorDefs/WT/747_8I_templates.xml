	</Template>

	<!-- IRS -->
	<Template Name="WT_B748_IRS_Knob_Template">
		<Parameters Type="Default">
			<ID>1</ID>
			<WWISE_PREFIX>standbypower_selector</WWISE_PREFIX>
		</Parameters>

		<Parameters Type="Default">
			<Switch Param="ID">
				<Case Value="2">
					<SIDE>c</SIDE>
				</Case>
				<Case Value="3">
					<SIDE>r</SIDE>
				</Case>
				<Default>
					<SIDE>l</SIDE>
				</Default>
			</Switch>
		</Parameters>

		<Parameters Type="Default">
			<IE_NAME>IRS_Switch_#ID#</IE_NAME>
			<NODE_ID>knob_ovhd_irs_#SIDE#</NODE_ID>
			<ANIM_NAME>ovhd_knob_irs_#SIDE#</ANIM_NAME>
		</Parameters>

		<Parameters Type="Override">
			<VAR_NAME>B748_IRS_Knob_State:#ID#</VAR_NAME>
		</Parameters>

		<UseTemplate Name="ASOBO_AIRLINER_Base_Template">
			<KNOB_TYPE>X_STATES</KNOB_TYPE>
			<INTERACTION_TYPE>Knob</INTERACTION_TYPE>
			<NUM_STATES>4</NUM_STATES>
			<STR_STATE_0>Off</STR_STATE_0>
			<STR_STATE_1>Align</STR_STATE_1>
			<STR_STATE_2>Nav</STR_STATE_2>
			<STR_STATE_3>Att</STR_STATE_3>
			<TT_VALUE_0>@TT_Package.GT_STATE_OFF</TT_VALUE_0>
			<TT_VALUE_1>@TT_Package.GT_STATE_ALIGN</TT_VALUE_1>
			<TT_VALUE_2>@TT_Package.GT_STATE_NAV</TT_VALUE_2>
			<TT_VALUE_3>@TT_Package.GT_STATE_ATT</TT_VALUE_3>
			<GET_STATE_EXTERNAL>(L:#VAR_NAME#)</GET_STATE_EXTERNAL>
			<SET_STATE_EXTERNAL>(O:AIRLINER_#IE_NAME#_Position) (&gt;L:#VAR_NAME#)</SET_STATE_EXTERNAL>
			<LOCAL_VAR_TO_WATCH_0>#VAR_NAME#</LOCAL_VAR_TO_WATCH_0>
			<TT_DESCRIPTION_ID>@TT_Package.AIRLINER_OVHD_KNOB_IRS_ACTION</TT_DESCRIPTION_ID>
			<TOOLTIP_TITLE>@TT_Package.AIRLINER_OVHD_KNOB_IRS_TITLE</TOOLTIP_TITLE>
			<EMISSIVE_TEMPLATE_TO_USE>ASOBO_GT_Emissive_Potentiometer</EMISSIVE_TEMPLATE_TO_USE>
		</UseTemplate>
	</Template>

	<!-- IRS Source Select -->
	<Template Name="WT_B748_IRS_Source_Knob_Template">
		<Parameters Type="Default">
			<ID>1</ID>
			<WWISE_PREFIX>standbypower_selector</WWISE_PREFIX>
		</Parameters>

		<Parameters Type="Default">
			<Switch Param="ID">
				<Case Value="2">
					<SIDE>r</SIDE>
				</Case>
				<Default>
					<SIDE>l</SIDE>
				</Default>
			</Switch>
		</Parameters>

		<Parameters Type="Default">
			<IE_NAME>IRS_Source_Switch_#ID#</IE_NAME>
			<NODE_ID>knob_source_select_irs_#SIDE#</NODE_ID>
			<ANIM_NAME>source_select_knob_irs_#SIDE#</ANIM_NAME>
		</Parameters>

		<Parameters Type="Override">
			<VAR_NAME>B748_IRS_Source_Knob_State:#ID#</VAR_NAME>

			<!--
				Lvar enum is L/C/R/AUTO.
				Pilot-side knob is L/AUTO/C/R. Copilot-side knob is L/C/AUTO/R
			-->
			<Switch Param="ID">
				<Case Value="2">
					<ENUM_VAL_TO_POS_EXTERNAL>2 3 1 0 4 l0 case</ENUM_VAL_TO_POS_EXTERNAL>
					<ENUM_POS_TO_VAL_EXTERNAL>2 3 1 0 4 l0 case</ENUM_POS_TO_VAL_EXTERNAL>
					<STR_STATE_1>Center</STR_STATE_1>
					<STR_STATE_2>Auto</STR_STATE_2>
					<TT_VALUE_1>@TT_Package.GT_STATE_CENTER</TT_VALUE_1>
					<TT_VALUE_2>@TT_Package.GT_STATE_AUTO</TT_VALUE_2>
				</Case>
				<Default>
					<ENUM_VAL_TO_POS_EXTERNAL>1 3 2 0 4 l0 case</ENUM_VAL_TO_POS_EXTERNAL>
					<ENUM_POS_TO_VAL_EXTERNAL>2 1 3 0 4 l0 case</ENUM_POS_TO_VAL_EXTERNAL>
					<STR_STATE_1>Auto</STR_STATE_1>
					<STR_STATE_2>Center</STR_STATE_2>
					<TT_VALUE_1>@TT_Package.GT_STATE_AUTO</TT_VALUE_1>
					<TT_VALUE_2>@TT_Package.GT_STATE_CENTER</TT_VALUE_2>
				</Default>
			</Switch>
		</Parameters>

		<UseTemplate Name="ASOBO_AIRLINER_Base_Template">
			<KNOB_TYPE>X_STATES</KNOB_TYPE>
			<INTERACTION_TYPE>Knob</INTERACTION_TYPE>
			<NUM_STATES>4</NUM_STATES>
			<STR_STATE_0>Left</STR_STATE_0>
			<STR_STATE_3>Right</STR_STATE_3>
			<TT_VALUE_0>@TT_Package.GT_STATE_LEFT</TT_VALUE_0>
			<TT_VALUE_3>@TT_Package.GT_STATE_RIGHT</TT_VALUE_3>
			<GET_STATE_EXTERNAL>(L:#VAR_NAME#) sp0</GET_STATE_EXTERNAL>
			<SET_STATE_EXTERNAL>(O:AIRLINER_#IE_NAME#_Position) sp0 #ENUM_POS_TO_VAL_EXTERNAL# (&gt;L:#VAR_NAME#)</SET_STATE_EXTERNAL>
			<LOCAL_VAR_TO_WATCH_0>#VAR_NAME#</LOCAL_VAR_TO_WATCH_0>
			<TT_DESCRIPTION_ID>@TT_Package.AIRLINER_KNOB_IRS_SOURCE_ACTION</TT_DESCRIPTION_ID>
			<TOOLTIP_TITLE>@TT_Package.AIRLINER_KNOB_IRS_SOURCE_TITLE</TOOLTIP_TITLE>
			<EMISSIVE_TEMPLATE_TO_USE>ASOBO_GT_Emissive_Potentiometer</EMISSIVE_TEMPLATE_TO_USE>
		</UseTemplate>
	</Template>

	<Template Name="WT_BOEING_Knob_Mins_Template">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<NODE_ID>BOEING_Knob_Mins_#ID#</NODE_ID>
			<ANIM_NAME>BOEING_Knob_Mins_#ID#</ANIM_NAME>
			<BUTTON_NODE_ID>BOEING_Knob_Mins_#ID#_Push</BUTTON_NODE_ID>
			<BUTTON_ANIM_NAME>BOEING_Knob_Mins_#ID#_Push</BUTTON_ANIM_NAME>
			<PART_ID>BOEING_Knob_Mins</PART_ID>
			<SYSTEM_NAME>PFD</SYSTEM_NAME>
			<WWISE_EVENT>autopilot_mins_knob</WWISE_EVENT>
			<WWISE_PREFIX>autopilot_mins_knob</WWISE_PREFIX>
			<TT_DESCRIPTION_ID>@TT_Package.AIRLINER_KNOB_BARO_KNOB_MINS_ACTION</TT_DESCRIPTION_ID>
			<TOOLTIP_TITLE>@TT_Package.AIRLINER_KNOB_BARO_KNOB_MINS_TITLE</TOOLTIP_TITLE>
			<TT_DESCRIPTION_ID_BUTTON>@TT_Package.AIRLINER_KNOB_BARO_PUSH_MINS_ACTION</TT_DESCRIPTION_ID_BUTTON>
			<TOOLTIP_TITLE_BUTTON>@TT_Package.AIRLINER_KNOB_BARO_PUSH_MINS_TITLE</TOOLTIP_TITLE_BUTTON>
		</Parameters>
		<Parameters Type="Override">
			<IE_NAME_KNOB>Mins_#ID#</IE_NAME_KNOB>
			<IE_NAME_BTN>Mins_Button_#ID#</IE_NAME_BTN>
		</Parameters>
		<UseTemplate Name="ASOBO_AIRLINER_Base_Template">
			<INTERACTION_TYPE>Knob</INTERACTION_TYPE>
			<IE_NAME>#IE_NAME_KNOB#</IE_NAME>
			<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#PLANE_PREFIX#_#SYSTEM_NAME#_Mins_INC) } els{ (&gt;H:#PLANE_PREFIX#_#SYSTEM_NAME#_Mins_DEC) }</SET_STATE_EXTERNAL>
			<IS_INFINITE>True</IS_INFINITE>
			<COUNT>36</COUNT>
		</UseTemplate>
		<UseTemplate Name="ASOBO_AIRLINER_Base_Template">
			<INTERACTION_TYPE>Push</INTERACTION_TYPE>
			<IE_NAME>#IE_NAME_BTN#</IE_NAME>
			<SET_STATE_EXTERNAL>if{ (&gt;H:#PLANE_PREFIX#_#SYSTEM_NAME#_Mins_Press) }</SET_STATE_EXTERNAL>
			<ANIM_NAME>#BUTTON_ANIM_NAME#</ANIM_NAME>
			<NODE_ID>#BUTTON_NODE_ID#</NODE_ID>
			<TT_DESCRIPTION_ID>#TT_DESCRIPTION_ID_BUTTON#</TT_DESCRIPTION_ID>
			<TOOLTIP_TITLE>#TOOLTIP_TITLE_BUTTON#</TOOLTIP_TITLE>
		</UseTemplate>
	</Template>

